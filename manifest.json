{
	"manifest_version": 2,
	"name": "Gen-z Attention Enhancer ",
	"description": "Base Level Extension",
	"version": "1.0",
	"browser_action": {
		"default_title": "Append Test Text"
	},

	"permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"css": ["player.css"]
		}
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}
